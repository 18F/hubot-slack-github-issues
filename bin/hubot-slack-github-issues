#! /usr/bin/env node

var path = require('path');

// Just going to wrap the pass through to slack-github-issues
var pkgPath = '../node_modules/slack-github-issues';
var pkgInfo = require(path.join(pkgPath, 'package.json'));
var binPath = pkgInfo.bin['slack-github-issues'];

require(path.join(pkgPath, binPath));
